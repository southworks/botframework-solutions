{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-microsoft-contrib"
    ],
    "linterOptions":{
      "exclude": [
        "src/resources/*"
      ]
    },
    "rules": {
      "linebreak-style": false,
      "member-ordering": false,
      "no-relative-imports": false,
      "export-name": false,
      "no-empty-interface": false 
    },
    "rulesDirectory": [
      "tslint-microsoft-contrib"
    ]
  }